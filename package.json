        "@semantic-release/npm",
        "@semantic-release/git"
    ],
    "scripts": {
        "storybook": "cross-env STORYBOOK_DISPLAY_WARNING=true DISPLAY_WARNING=true start-storybook -p 3001 -s ./static --no-dll",
        "start": "start-storybook -p 3001 -s ./static",
        "build-storybook": "build-storybook",
        "stylelint": "npx stylelint \"**/*.{scss}\"",
        "stylelint:fix": "npx stylelint \"**/*.{scss}\" --fix",
        "build": "rollup -c",
        "lint": "stylelint \"src/**/*.scss\" && eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx src/",
        "lint:fix": "stylelint \"src/**/*.scss\" --fix && eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
        "lint-quiet": "eslint --ext .js,.jsx,.ts,.tsx src/ --quiet",
        "test": "jest --config jest.config.js --coverage --maxWorkers=2",
        "update-snap": "jest --updateSnapshot",
        "build-watch": "rollup -c -w"
    }
